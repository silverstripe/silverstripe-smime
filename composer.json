{
    "name": "silverstripe/smime",
    "description": "Provides support for using S/MIME with the standard Silverstripe Email API.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "smime",
        "email"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.5"
    },
    "authors": [
        {
            "name": "Frank Mullenger",
            "email": "frank@silverstripe.com"
        },
        {
            "name": "Danae Miller-Clendon",
            "email": "danae.millerclendon@silverstripe.com"
        }
    ],
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SMIME\\": "src/",
            "SilverStripe\\SMIME\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
